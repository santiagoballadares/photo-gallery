{
	"name": "photo-gallery",
	"description": "Optimized Virtualized Masonry Grid with Detailed Photo View",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"engines": {
		"node": ">=18.20.8"
	},
	"scripts": {
		"check": "npm run check:eslint && npm run check:prettier",
		"check:eslint": "eslint --cache --cache-strategy content --max-warnings=0",
		"check:prettier": "prettier --check .",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"fix": "npm run fix:eslint && npm run fix:prettier",
		"fix:eslint": "eslint --cache --cache-strategy content --max-warnings=0 --fix .",
		"fix:prettier": "prettier --write .",
		"preview": "vite preview",
		"test": "vitest"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"pexels": "^1.4.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.6.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.25.0",
		"@tailwindcss/vite": "^4.1.8",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/lodash": "^4.17.17",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react-swc": "^3.9.0",
		"eslint": "^9.25.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"tailwindcss": "^4.1.8",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.3.5",
		"vitest": "^3.2.3"
	}
}
